    Alert.alert(
      'Log Out', 'Are you sure you\'d like to log out?',
      [
        { text: 'Cancel', style: 'cancel' },
        {
          text: 'Log Out',
          style: 'destructive',
          onPress: async () => {
                          try {
                              Alert.alert('Logged Out', 'You have successfully logged out');
                          } catch (error) {
                              console.error('Error during logout:', error);
                          }
                      },
                    },
                  ]
                );